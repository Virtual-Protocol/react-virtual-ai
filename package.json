    "eslint-plugin-react-refresh": "^0.4.5",
    "glob": "^10.3.10",
    "postcss": "^8.4.35",
    "standard-version": "^9.5.0",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.2.2",
    "vite": "^5.1.0",
    "vite-plugin-dts": "^3.7.2",
    "vite-plugin-lib-inject-css": "^1.3.0"
