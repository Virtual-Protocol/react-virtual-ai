    "@react-three/fiber": "^8.15.16",
    "framer-motion": "^11.0.5",
    "gsap": "^3.12.5",
    "jwt-decode": "^4.0.0",
    "mmd-parser": "^1.0.4",
    "react": "^18.2.0",
    "react-audio-voice-recorder": "^2.2.0",
    "react-dom": "^18.2.0",
